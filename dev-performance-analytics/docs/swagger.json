{
    "swagger": "2.0",
    "info": {
        "description": "This is a developer performance analytics server.",
        "title": "Developer Performance Analytics API",
        "termsOfService": "http://swagger.io/terms/",
        "contact": {
            "name": "API Support",
            "url": "http://www.swagger.io/support",
            "email": "support@swagger.io"
        },
        "license": {
            "name": "Apache 2.0",
